{
  "name": "entrada-dizomos-ofertas",
  "version": "1.0.0",
  "description": "Sistema de Entrada de Dizimos e Ofertas",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-test --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "package-linux": "electron-packager . electron-test --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flaviotsx/entrance-of-tithes-and-offerings.git"
  },
  "author": "Flavio Tavares",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.1.1",
    "electron": "^1.8.4",
    "ipc": "0.0.1",
    "path": "^0.12.7",
    "pdfreader": "^0.2.5",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "electron-packager": "^12.0.1"
  },
  "bugs": {
    "url": "https://github.com/flaviotsx/entrance-of-tithes-and-offerings/issues"
  },
  "homepage": "https://github.com/flaviotsx/entrance-of-tithes-and-offerings#readme"
}
